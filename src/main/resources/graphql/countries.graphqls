scalar Date

type State {
    id: ID!
    name: String!
    countryId: ID!
}

type Country {
    id: ID!
    name: String!
}

type Countries {
    Country: Country
}

type Query {
    findAllCountries: [Country]!
    findAllStates: [State]!
}

type Mutation {
    newCountry(name: String!) : Country!
    saveAllCountries(names: [String!]!): [String]!
}
